<%= render 'user', book: @book, user: @user %>
<div class="col-md3">
<h3>Users</h3>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
<tbody>
  <% @users.each do |user| %>
   <tr>
     <td><%=image_tag user.profile_image %></td>
     <td><%=user.name %></td>
     <td><%= link_to 'Show', user_path(user.id)%></td>
   </tr>
 <% end %>
   </tbody>
 </table>
</div>
</div>
</div>